    "definitions": {
        "internal_kinetria_gateways_http.AbandonSessionRequest": {
            "type": "object",
            "properties": {
                "notes": {
                    "type": "string",
                    "example": "Precisei sair mais cedo"
                }
            }
        },
        "internal_kinetria_gateways_http.AuthResponse": {
            "type": "object",
            "properties": {
                "accessToken": {
                    "type": "string",
                    "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
                },
                "expiresIn": {
                    "type": "integer",
                    "example": 3600
                },
                "refreshToken": {
                    "type": "string",
                    "example": "abc123def456..."
                }
            }
        },
        "internal_kinetria_gateways_http.DashboardResponse": {
            "type": "object",
            "properties": {
                "stats": {
                    "$ref": "#/definitions/internal_kinetria_gateways_http.WeekStats"
                },
                "todayWorkout": {
                    "$ref": "#/definitions/internal_kinetria_gateways_http.TodayWorkout"
                },
                "user": {
                    "$ref": "#/definitions/internal_kinetria_gateways_http.DashboardUser"
                },
                "weekProgress": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/internal_kinetria_gateways_http.DayProgress"
                    }
                }
            }
        },
        "internal_kinetria_gateways_http.DashboardUser": {
            "type": "object",
            "properties": {
                "email": {
                    "type": "string",
                    "example": "bruno@example.com"
                },
                "id": {
                    "type": "string",
                    "example": "b67a784a-e54b-4330-9b6b-8dd930c4a746"
                },
                "name": {
                    "type": "string",
                    "example": "Bruno Costa"
                },
                "profileImageUrl": {
                    "type": "string",
                    "example": "/assets/avatars/default.png"
                }
            }
        },
        "internal_kinetria_gateways_http.DayProgress": {
            "type": "object",
            "properties": {
                "date": {
                    "type": "string",
                    "example": "2026-02-25"
                },
                "day": {
                    "type": "string",
                    "example": "Q"
                },
                "status": {
                    "type": "string",
                    "enum": [
                        "completed",
                        "missed",
                        "future"
                    ],
                    "example": "completed"
                }
            }
        },
        "internal_kinetria_gateways_http.ErrorResponse": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string",
                    "example": "VALIDATION_ERROR"
                },
                "message": {
                    "type": "string",
                    "example": "Invalid request body"
                }
            }
        },
        "internal_kinetria_gateways_http.FinishSessionRequest": {
            "type": "object",
            "properties": {
                "notes": {
                    "type": "string",
                    "example": "Treino completo! Ótima performance."
                }
            }
        },
        "internal_kinetria_gateways_http.LoginRequest": {
            "type": "object",
            "required": [
                "email",
                "password"
            ],
            "properties": {
                "email": {
                    "type": "string",
                    "example": "bruno@example.com"
                },
                "password": {
                    "type": "string",
                    "example": "Password123!"
                }
            }
        },
        "internal_kinetria_gateways_http.LogoutRequest": {
            "type": "object",
            "required": [
                "refreshToken"
            ],
            "properties": {
                "refreshToken": {
                    "type": "string",
                    "example": "abc123def456..."
                }
            }
        },
        "internal_kinetria_gateways_http.MessageResponse": {
            "type": "object",
            "properties": {
                "message": {
                    "type": "string",
                    "example": "Logged out successfully"
                }
            }
        },
        "internal_kinetria_gateways_http.PaginationMeta": {
            "type": "object",
            "properties": {
                "limit": {
                    "type": "integer",
                    "example": 10
                },
                "page": {
                    "type": "integer",
                    "example": 1
                },
                "total": {
                    "type": "integer",
                    "example": 25
                },
                "totalPages": {
                    "type": "integer",
                    "example": 3
                }
            }
        },
        "internal_kinetria_gateways_http.RecordSetRequest": {
            "type": "object",
            "required": [
                "exerciseId",
                "reps",
                "setNumber",
                "status"
            ],
            "properties": {
                "exerciseId": {
                    "type": "string",
                    "example": "e1f2g3h4-i5j6-7890-abcd-ef1234567890"
                },
                "reps": {
                    "type": "integer",
                    "minimum": 0,
                    "example": 12
                },
                "setNumber": {
                    "type": "integer",
                    "minimum": 1,
                    "example": 1
                },
                "status": {
                    "type": "string",
                    "enum": [
                        "completed",
                        "skipped"
                    ],
                    "example": "completed"
                },
                "weight": {
                    "type": "number",
                    "minimum": 0,
                    "example": 80.5
                }
            }
        },
        "internal_kinetria_gateways_http.RecordSetResponse": {
            "type": "object",
            "properties": {
                "exerciseId": {
                    "type": "string",
                    "example": "e1f2g3h4-i5j6-7890-abcd-ef1234567890"
                },
                "reps": {
                    "type": "integer",
                    "example": 12
                },
                "setNumber": {
                    "type": "integer",
                    "example": 1
                },
                "setRecordId": {
                    "type": "string",
                    "example": "s1t2u3v4-w5x6-7890-abcd-ef1234567890"
                },
                "status": {
                    "type": "string",
                    "example": "completed"
                },
                "weight": {
                    "type": "number",
                    "example": 80.5
                }
            }
        },
        "internal_kinetria_gateways_http.RefreshResponse": {
            "type": "object",
